{
  "name": "Auto Forward",
  "description": "Auto forward messages from chats to others with your Telegram user account",
  "keywords": [
    "forward",
    "telethon",
    "bot",
    "telegram"
  ],
  "repository": "https://github.com/kd2929/MergeBot/",
  "success_url": "/welcome",
  "env": {
      "API_ID": {
        "description": "Get this value from https://my.telegram.org/apps",
        "value": "",
        "required": true
      },
      "API_HASH": {
        "description": "Get this value from https://my.telegram.org/apps",
        "value": "",
        "required": true
      },
      "STRING_SESSION": {
        "description": "Get this value by running https://repl.it/@KeselekPermen/UserButt#main.py",
        "required": true
      }
  },
  "formation": {
      "worker": {
        "quantity": 1,
        "size": "free"
      }
    },
  "image": "heroku/python",
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ]
}
